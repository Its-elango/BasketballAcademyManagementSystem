@model IEnumerable<BasketballAcademy.Models.Registeration>

@{
    ViewBag.Title = "ViewRegistration";
    Layout = "~/Views/Shared/AdminLayoutPage.cshtml";
}
<script src="~/Scripts/PreventBack.js"></script>
<h2 class="text-center">Registred list</h2>


<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-lg-4 col-md-6 mb-4">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">@Html.DisplayFor(modelItem => item.Name)</h5>
                 
                </div>
              
            </div>
        </div>
    }
</div>
